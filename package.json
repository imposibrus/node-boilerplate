{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "gulp build",
    "start": "node ./dst/bin/www",
    "test": "PORT=0 NEW_RELIC_ENABLED=false NODE_ENV=testing mocha test/*.ts",
    "coverage": "PORT=0 NEW_RELIC_ENABLED=false nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./public/coverage",
    "temp-directory": "./public/coverage"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 9"
  ],
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "^1.17.2",
    "connect-flash": "0.1.1",
    "connect-redis": "3.3.0",
    "cookie-parser": "^1.4.3",
    "debug": "^3.0.1",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "fs-extra": "^4.0.2",
    "intel": "^1.1.2",
    "ioredis": "^3.1.4",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "nconf": "^0.8.4",
    "newrelic": "2.2.1",
    "noty": "3.1.0",
    "nunjucks": "3.0.1",
    "pg": "6.2.2",
    "pg-hstore": "2.3.2",
    "post-normalize": "0.0.4",
    "require-all": "^2.2.0",
    "sequelize": "3.30.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/connect-flash": "0.0.31",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "0.0.29",
    "@types/express-session": "0.0.32",
    "@types/ioredis": "0.0.23",
    "@types/jquery": "^2.0.45",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/nconf": "0.0.34",
    "@types/node": "^7.0.22",
    "@types/nunjucks": "0.0.32",
    "@types/redis": "^0.12.36",
    "@types/sequelize": "^4.0.55",
    "@types/webpack": "^2.2.15",
    "@types/webpack-hot-middleware": "^2.15.0",
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer-stylus": "^0.14.0",
    "awesome-typescript-loader": "3.1.3",
    "browser-sync": "^2.18.12",
    "css-loader": "^0.28.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "gulp": "4.0.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "3.1.6",
    "mocha": "3.5.3",
    "normalize.css": "^7.0.0",
    "nyc": "^10.3.2",
    "pluralize": "1.2.1",
    "should": "13.0.1",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "supertest": "^3.0.0",
    "ts-node": "3.0.4",
    "typescript": "2.3.3",
    "url-loader": "0.5.9",
    "webpack": "3.6.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-module-hot-accept": "^1.0.5"
  }
}
