{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "gulp build",
    "start": "node ./dst/bin/www",
    "test": "cross-env PORT=0 NODE_ENV=testing mocha test/**/*.ts",
    "coverage": "cross-env PORT=0 NODE_ENV=testing nyc mocha",
    "package": "pkg --out-path build ."
  },
  "bin": "dst/bin/www.js",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./coverage",
    "temp-directory": "./coverage"
  },
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "scripts": [
      "node_modules/sequelize-typescript/lib/models/v4/**/*.js",
      "migrations/*.js"
    ],
    "targets": [
      "node8-linux-x64"
    ]
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.3",
    "debug": "3.1.0",
    "express": "4.16.3",
    "fs-extra": "6.0.1",
    "intel": "1.2.0",
    "ioredis": "3.2.2",
    "lodash": "4.17.10",
    "morgan": "1.9.0",
    "pg": "6.4.2",
    "pg-hstore": "2.3.2",
    "post-normalize": "0.0.4",
    "require-all": "^2.2.0",
    "sequelize": "4.37.10",
    "sequelize-typescript": "0.6.6-beta.1",
    "umzug": "2.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/debug": "0.0.30",
    "@types/dotenv": "4.0.3",
    "@types/express": "4.16.0",
    "@types/fs-extra": "5.0.3",
    "@types/ioredis": "3.2.8",
    "@types/lodash": "4.14.109",
    "@types/mocha": "5.2.2",
    "@types/morgan": "^1.7.33",
    "@types/node": "10.3.3",
    "@types/pg": "6.1.47",
    "@types/redis": "2.8.6",
    "@types/sequelize": "4.27.21",
    "@types/supertest": "2.0.4",
    "@types/umzug": "2.1.1",
    "cross-env": "5.2.0",
    "dotenv": "6.0.0",
    "gulp": "4.0.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-notify": "3.2.0",
    "gulp-plumber": "1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "4.0.2",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "pkg": "4.3.1",
    "reflect-metadata": "0.1.12",
    "should": "13.2.1",
    "source-map-support": "0.5.6",
    "supertest": "3.1.0",
    "ts-node": "6.1.1",
    "typescript": "2.9.2"
  }
}
